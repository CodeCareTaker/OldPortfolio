<% include ../partials/landingHeader %>

<div class="container">
    <div class="row">
        <h1 class="newText">Edit Project Details</h1>
        <div class="formContainer">
            <form action="/photos" method="POST">
                <div class="form-group">
                    <input class="form-control" type="text" name="project[title]" value="<%= project.title %>">
                </div>
                <div class="form-group">
                    <input class="form-control" type="text" name="project[image]" value="<%= project.image %>">
                </div>
                <div class="form-group">
                    <input class="form-control" type="text" name="project[image2]" value="<%= project.image2 %>">
                </div>
                <div class="form-group">
                    <input class="form-control" type="text" name="project[image3]" value="<%= project.image3 %>">
                </div>
                <div class="form-group">
                    <input class="form-control" type="text" name="project[description]" value="<%= project.description %>">
                </div>
                <div class="form-group">
                    <button class="btn btn-lg btn-primary btn-block">Submit</button>
                </div>
            </form>
            <a href="/projects" class="newText">Go Back</a>
        </div>
    </div>
</div>

<% include ../partials/footer %>